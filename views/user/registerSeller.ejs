<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
  </head>
  <body>
    <form action="/user/register/seller" method="POST">
      <div class="form-group mb-3">
        <label for="exampleInputEmail1">아이디</label>
        <input type="text" class="form-control" name="id" aria-describedby="emailHelp" value="<%= (typeof formData !== 'undefined' && formData.id) ? formData.id : '' %>" placeholder="아이디를 입력해주세요." required>
        <% if (typeof errors !== 'undefined' && errors.id) { %>
          <span id="idError" class="error-message text-danger"><%= errors.id %></span>
        <% } %>
      </div>
      <div class="form-group mb-3">
        <label for="exampleInputPassword1">비밀번호</label>
        <input type="password" class="form-control" name="password" value="<%= (typeof formData !== 'undefined' && formData.password) ? formData.password : '' %>" placeholder="비밀번호를 입력해주세요." required>
      </div>
      <div class="form-group mb-3">
        <label for="exampleInputPassword1">비밀번호 확인</label>
        <input type="password" class="form-control" name="password2" value="<%= (typeof formData !== 'undefined' && formData.password2) ? formData.password2 : '' %>" placeholder="비밀번호를 확인해주세요." required>
        <% if (typeof errors !== 'undefined' && errors.password2) { %>
          <span id="idError" class="error-message text-danger"><%= errors.password2 %></span>
        <% } %>
      </div>
      <div class="form-group mb-3">
        <label for="exampleInputPassword1">가게 이름</label>
        <input type="text" class="form-control" name="storeName" value="<%= (typeof formData !== 'undefined' && formData.storeName) ? formData.storeName : '' %>" placeholder="가게 이름을 입력해주세요." required>
      </div>
      <div class="form-group mb-3">
        <label for="exampleInputPassword1">소유자 이름</label>
        <input type="text" class="form-control" name="ownerName" value="<%= (typeof formData !== 'undefined' && formData.ownerName) ? formData.ownerName : '' %>" placeholder="소유자 이름을 입력해주세요." required>
      </div>
      <div class="form-group mb-3">
        <label for="exampleInputPassword1">사업자 등록번호</label>
        <input type="text" class="form-control" name="businessNumber" value="<%= (typeof formData !== 'undefined' && formData.businessNumber) ? formData.businessNumber : '' %>" placeholder="사업자 등록번호를 입력해주세요." required>
      </div>
      <div class="form-group mb-3">
        <label for="exampleInputPassword1">가게 전화번호</label>
        <input type="text" class="form-control" name="storePhoneNumber" value="<%= (typeof formData !== 'undefined' && formData.storePhoneNumber) ? formData.storePhoneNumber : '' %>" placeholder="가게 전화번호를 입력해주세요." required>
      </div>
      <div class="form-group mb-3">
        <label for="exampleInputPassword1">주소</label>
        <br>
        <input type="text" class="form-control" name="address_city" value="<%= (typeof formData !== 'undefined' && formData.address_city) ? formData.address_city : '' %>" placeholder="시/도" required>
        <input type="text" class="form-control" name="address_district" value="<%= (typeof formData !== 'undefined' && formData.address_district) ? formData.address_district : '' %>" placeholder="시/군/구" required>
        <input type="text" class="form-control" name="address_dong" value="<%= (typeof formData !== 'undefined' && formData.address_dong) ? formData.address_dong : '' %>" placeholder="읍/면/동" required>
      </div>
      <div class="form-group mb-3">
        <label for="exampleInputPassword1">주소(상세주소)</label>
        <input type="text" class="form-control" name="address_detail" value="<%= (typeof formData !== 'undefined' && formData.address_detail) ? formData.address_detail : '' %>" placeholder="주소(상세주소)를 입력해주세요." required>
      </div>
      <button type="submit" class="btn btn-primary form-control">가입하기</button>
    </form>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-kenU1KFdBIe4zVF0s0G1M5b4hcpxyD9F7jL+jjXkk+Q2h455rYXK/7HAuoJl+0I4" crossorigin="anonymous"></script>
  </body>
</html>